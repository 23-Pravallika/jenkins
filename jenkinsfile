pipeline {
    agent any 
    
    stages{
        stage('stage Name-1'){
            steps{
                sh "echo I am using the Pipeline Syntax Help"
            }
        }

        stage('stage Name-2'){
            steps{
                sh "echo this is pipeline"
            }
        }

        stage('stage Name'){
            steps{
                sh '''  echo This is demo on pipeline
                        echo  this is stage-1
                        echo  this is stage-2
                        echo  this is stage-3 '''
            }
        }
    }
}

